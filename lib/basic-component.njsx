<App>
    const [count, setCount] = React.useState(0)
    const ref = React.useRef()

    React.useEffect(()=>{
        setCount(prev => prev + 1)
    }, [])

    React.useEffect(()=>{
        console.log(ref.current)
    }, [ref.current])
    // ref.current

    return @(
        <div>
            <input ref={ref} type="number" value={count} />
        </div>
    )@
</App>